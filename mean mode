data <- c(13, 15, 16, 16, 19, 20, 20, 21, 22, 22, 25, 25, 25, 25, 30, 33, 33, 35, 35, 35, 35, 36, 40, 45, 46, 52, 70)
result.mean <- mean(data)
result.median <- median(data)
result.mode <- mode(data)
result.quantile1 <- quantile(data ,0.25)
result.quantile3 <- quantile(data ,0.75)
print(result.mean)
print(result.median)
print(result.mode)
print(result.quantile1)
print(result.quantile3)
